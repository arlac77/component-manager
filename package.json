{
  "name": "component-manager",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/component-manager.js",
  "module": "src/component-manager.mjs",
  "description": "manages components and its dependencies",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "component-manager": "bin/component-manager"
  },
  "scripts": {
    "cover": "c8 -x tests/** --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/component-manager.mjs --section=API",
    "lint": "documentation lint src/component-manager.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.0.15",
    "commander": "^2.20.0",
    "config-expander": "^10.2.5",
    "github-repository-provider": "^7.0.9",
    "pacote": "^9.1.0"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "builtin-modules": "^3.1.0",
    "c8": "^5.0.2",
    "documentation": "^12.0.1",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-executable": "^1.5.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.19"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/component-manager.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/component-manager/issues"
  },
  "homepage": "https://github.com/arlac77/component-manager#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
