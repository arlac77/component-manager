{
  "name": "component-manager",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/component-manager.js",
  "module": "src/component-manager.mjs",
  "description": "manages components and its dependencies",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "component-manager": "bin/component-manager"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/component-manager.mjs --section=API",
    "lint": "documentation lint src/component-manager.mjs",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "aggregation-repository-provider": "^2.0.15",
    "caporal": "^0.10.0",
    "config-expander": "^9.1.8",
    "github-repository-provider": "^4.0.0",
    "pacote": "^9.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
    "ava": "^1.0.0-rc.1",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.1.0",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.10.7",
    "travis-deploy-once": "^5.0.9"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/component-manager.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/component-manager/issues"
  },
  "homepage": "https://github.com/arlac77/component-manager#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
